{
  "extends": "@workspace/typescript-config/base.json",
  "include": ["src"],
  "compilerOptions": {
    "target": "ES2016",
    "module": "nodenext",
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true
  },
  "exclude": ["node_modules", "dist"]
}
